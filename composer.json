{
    "name": "michielkempen/laravel-http-responses",
    "description": "A set of helper classes to segregate the transformation logic from controller to separate dedicated classes.",
    "keywords": [
        "Michiel Kempen",
        "Laravel HTTP responses"
    ],
    "homepage": "https://github.com/michielkempen/laravel-http-responses",
    "license": "MIT",
    "authors": [
        {
            "name": "Michiel Kempen",
            "email": "kempenmichiel@gmail.com",
            "homepage": "https://michielkempen.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "illuminate/http": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "MichielKempen\\LaravelHttpResponses\\": "src"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
